    </joint>
    <joint name="${prefix}joint_5" type="revolute">
      <axis xyz="0.0 0.0 1.0"/>
      <child link="${prefix}link_5"/>
      <limit effort="1398.30298" lower="-2.181661564992912E0" upper="2.181661564992912E0" velocity="${radians(172)}"/>
