      <limit effort="2274.29199" lower="-6.1086523819801535E0" upper="6.1086523819801535E0" velocity="${radians(179)}"/>
      <origin rpy="1.5707963267948963 0.0 -0.0" xyz="0.115 0.0 0.0"/>
