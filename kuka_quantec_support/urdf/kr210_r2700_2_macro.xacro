        <geometry>
          <mesh filename="package://kuka_quantec_support/meshes/kr210_r2700_2/collision/link_2.stl"/>
        </geometry>
        <origin rpy="-0.0 0.0 -0.0" xyz="0.0 0.0 0.0"/>
