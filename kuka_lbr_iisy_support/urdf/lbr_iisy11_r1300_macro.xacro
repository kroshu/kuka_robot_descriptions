        <mass value="7.49283573690527E0"/>
        <origin rpy="0.1692981167548117 0.5271433699021747 -2.9605806714357508" xyz="0.0438696763639649 -0.00482189531013714 0.044913251068375"/>
      </inertial>
      <visual>
