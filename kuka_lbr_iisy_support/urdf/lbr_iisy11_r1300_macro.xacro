          <mesh filename="package://kuka_lbr_iisy_support/meshes/lbr_iisy11_r1300/collision/base_link.stl"/>
        </geometry>
        <origin rpy="-0.0 0.0 -0.0" xyz="0.0 0.0 0.0"/>
